{
  "cli": {
    "version": ">=7.3.0",
    "appVersionSource": "remote"
  },
  "build": {
    "fast": {
      "ios": {
        "resourceClass": "m-medium"
      },
      "channel": "fast"
    },
    "development": {
      "extends": "fast",
      "distribution": "internal",
      "developmentClient": true,
      "channel": "default",
      "environment": "development"
    },
    "staging": {
      "distribution": "internal",
      "extends": "fast",
      "channel": "staging",
      "autoIncrement": true,
      "environment": "preview"
    },
    "testing": {
      "distribution": "internal",
      "extends": "fast",
      "channel": "testing",
      "autoIncrement": true,
      "environment": "development"
    },
    "production": {
      "extends": "fast",
      "channel": "production",
      "autoIncrement": true,
      "environment": "production",
      "ios": {
        "image": "macos-sequoia-15.3-xcode-16.2"
      }
    }
  }
}
